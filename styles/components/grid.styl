.container
  height 100%
  box-sizing border-box
  container-fixed()
  @extends .clearfix

  @media (min-width: $screen-sm-min)
    width $container-sm
  @media (min-width: $screen-md-min)
    width $container-md
  @media (min-width: $screen-lg-min)
    width $container-lg

.container-lg
  @media (min-width: $screen-sm-min)
    width $container-sm
  @media (min-width: $screen-md-min)
    width $container-md
  @media (min-width: $screen-lg-min)
    width 1100px

.container-fluid
  container-fixed()
  @extends .clearfix
  box-sizing border-box

.container-fluid
  width 1400px
  max-width 100%
  height 100%

.row
  make-row()
  @extends .clearfix

make-grid-columns()


make-grid(xs)


@media (min-width: $screen-sm-min)
  make-grid(sm)


@media (min-width: $screen-md-min)
  make-grid(md)


@media (min-width: $screen-lg-min)
  make-grid(lg)
